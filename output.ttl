@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://data.europa.eu/m8g/ContactPoint> a owl:Class .

<http://data.europa.eu/m8g/GenericDate> a owl:Class .

<http://data.europa.eu/m8g/contactPage> a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "contact page"@en ;
    rdfs:comment "A web page that could be used to reach out the Contact Point."@en ;
    rdfs:isDefinedBy <http://data.europa.eu/m8g> .

<http://data.europa.eu/m8g/contactPoint> a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "contact point"@en ;
    rdfs:comment "The main contact information of the resource."@en ;
    rdfs:isDefinedBy <http://data.europa.eu/m8g> .

<http://data.europa.eu/m8g/domicile> a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "domicile"@en ;
    rdfs:comment "The place that the Person treats as permanent home."@en ;
    rdfs:isDefinedBy <http://data.europa.eu/m8g> .

<http://data.europa.eu/m8g/gender> a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "gender"@en ;
    rdfs:comment "The identities, expressions and societal roles of the Person."@en ;
    rdfs:isDefinedBy <http://data.europa.eu/m8g> .

<http://data.europa.eu/m8g/matronymicName> a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:label "matronymic name"@en ;
    rdfs:comment "Name based on the given name of the Person's mother."@en ;
    rdfs:isDefinedBy <http://data.europa.eu/m8g> .

<http://data.europa.eu/m8g/sex> a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "sex"@en ;
    rdfs:comment "The organism's biological sex."@en ;
    rdfs:isDefinedBy <http://data.europa.eu/m8g> .

